{
  "name": "unbuild",
  "version": "3.0.0-rc.1",
  "description": "A unified javascript build system",
  "repository": "unjs/unbuild",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "unbuild": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm unbuild",
    "dev": "pnpm unbuild test/fixture",
    "lint": "eslint --ext .ts,.js . && prettier -c src test",
    "lint:fix": "eslint --fix --ext .ts,.js . && prettier -w src test",
    "prepack": "pnpm unbuild",
    "release": "pnpm test && changelogen --release && git push --follow-tags && npm publish",
    "prerelease": "pnpm test && changelogen --prerelease --release && git push --follow-tags && npm publish --tag rc",
    "stub": "pnpm unbuild --stub",
    "test": "pnpm lint && vitest run --coverage",
    "unbuild": "jiti ./src/cli"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/pluginutils": "^5.1.0",
    "chalk": "^5.3.0",
    "citty": "^0.1.5",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "esbuild": "^0.20.0",
    "globby": "^14.0.0",
    "hookable": "^5.5.3",
    "jiti": "^1.21.0",
    "magic-string": "^0.30.6",
    "mkdist": "^1.4.0",
    "mlly": "^1.5.0",
    "pathe": "^1.1.2",
    "pkg-types": "^1.0.3",
    "pretty-bytes": "^6.1.1",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "scule": "^1.2.0",
    "ufo": "^1.3.2",
    "untyped": "^1.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.14",
    "@vitest/coverage-v8": "^1.2.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.15.1"
}
